{
  "expo": {
    "name": "PedalSteelApp",
    "slug": "PedalSteelApp",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/images/icon.png",
    "scheme": "pedalsteelapp",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#25292e",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#25292e"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.wirttplb.PedalSteelApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#25292e"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ac74970a-c11e-48e9-8594-37c78e0f9519"
      }
    }
  }
}
